{
  "name": "server.boilerplate",
  "version": "5.2.2",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "cleanInstallPackages": "rm -rf node_modules package-lock.json && npm install",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "api-test": "mocha test/ApiTesting/tests/**/**.spec.mjs --reporter mochawesome || true",
    "api-test-and-cleanup": "npm run api-test && node test/ApiTesting/cleanup.js",
    "api-report": "open mochawesome-report/mochawesome.html",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "documentation:serve": "npx @compodoc/compodoc -p tsconfig.build.json --serve --watch",
    "generate-s3-assets-list": "ts-node -r tsconfig-paths/register src/utils/S3/s3_bucket_separation_script/generate-s3-assets-list.ts",
    "prepare": "husky install",
    "rimraf": "./node_modules/rimraf/bin.js"
  },
